#include <stdio.h>
int main() {
    int T, N;
    printf("enter the no.of test cases");
    scanf("%d", &T);
    for (int i = 0; i < T; i++) {
        printf("no. of bits");
        scanf("%d", &N);
        if (N % 4 == 0) {
            printf("Good\n");
        } else {
            printf("Not Good\n");
        }
    }
}
