#include <stdio.h>
int main() {
    int t;
    printf("number of test cases");
    scanf("%d",&t);
    for (int i=0;i<t;i++){
        int x,y;
        printf("enter the time taken by bike" );
        scanf("%d",&x);
        printf("enter the time taken by car" );
        scanf("%d",&y);
        if(x<y){
            printf("BIKE\n");
        }else if (x>y){
            printf("CAR\n");
        }else{
            printf("SAME\n");
        }
    }

    return 0;
}
